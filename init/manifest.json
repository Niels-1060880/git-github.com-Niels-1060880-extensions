{
    "manifest_version": 2,
    "name": "Initative",
    "version": "1.0",
  
    "description": "A firefox extension to manage turns in DnD combat",
    "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/beastify",
    "icons": {
      "48": "icons/d20-mimic-48.png"
    },
  
    "permissions": [
      "activeTab"
    ],
  
    "browser_action": {
      "default_icon": "icons/d20-mimic-32.png",
      "default_title": "Beastify",
      "default_popup": "popup/choose_beast.html"
    },
  
    "web_accessible_resources": [
      "beasts/frog.jpg",
      "beasts/turtle.jpg",
      "beasts/snake.jpg"
    ]
  }
  